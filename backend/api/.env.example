# Server Configuration
PORT=3001
NODE_ENV=development

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/medplatform
DATABASE_SSL=false

# JWT
JWT_SECRET=your-jwt-secret-min-32-chars-change-in-production
JWT_EXPIRATION=1h

# Redis (Session Management)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_KMS_KEY_ID=your-kms-key-id
AWS_S3_BUCKET=medrx-prescriptions

# Encryption
ENCRYPTION_KEY=your-aes-256-encryption-key-32-bytes

# External APIs
NIH_API_KEY=your-nih-api-key
FHIR_BASE_URL=https://fhir.example.com
DEA_VERIFICATION_API=https://dea-api.example.com

# Rate Limiting
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX=100
